<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.com/xml/ns/pro"
        xsi:schemaLocation="
       http://www.liquibase.org/xml/ns/dbchangelog
       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
       http://www.liquibase.com/xml/ns/pro
       https://www.liquibase.com/xml/ns/pro/liquibase-pro-latest.xsd">

<changeSet id="create-robot-labels-table" author="yourname">
    <createTable tableName="robot_labels">
        <column name="user_name" type="CHAR(25)"/>
        <column name="reporting_date" type="SMALLDATETIME"/>
        <column name="bundles" type="INT"/>
        <column name="individual_labels" type="INT"/>
        <column name="robot_label_total" type="INT"/>
    </createTable>
</changeSet>

</databaseChangeLog>
