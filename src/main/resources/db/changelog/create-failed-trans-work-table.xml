<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create-failed-trans-work-table" author="Edward jing">
        <createTable tableName="FAILED_TRANS_WORK">
            <column name="case_number" type="CHAR(12)"/>
            <column name="pi_id" type="CHAR(16)"/>
            <column name="account" type="CHAR(16)"/>
            <column name="date_time" type="DATETIME"/>
            <column name="non_mon" type="INT"/>
            <column name="retry_count" type="INT"/>
            <column name="message" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
