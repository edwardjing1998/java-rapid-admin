<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
      http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="001-create-bulk-card-table" author="Edward Jing">
        <createTable tableName="bulk_card">
            <column name="case_number" type="CHAR(12)">
                <constraints nullable="false"/>
            </column>
            <column name="PI_ID" type="CHAR(16)">
                <constraints nullable="false"/>
            </column>
            <column name="Bulk_PI_ID" type="CHAR(16)">
                <constraints nullable="false"/>
            </column>
            <column name="in_date" type="DATETIME">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
