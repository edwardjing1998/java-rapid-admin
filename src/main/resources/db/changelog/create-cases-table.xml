        <column name="msg_id" type="char(40)"/>
        <column name="ml_mthd" type="char(30)"/>
        <column name="source_file" type="char(16)"/>
        <column name="cust_id" type="char(9)"/>
        <column name="ms_issue_date" type="char(5)"/>
        <column name="cust_id2" type="char(9)"/>
        <column name="mkt_cd" type="char(2)"/>
        <column name="account_tokenid" type="char(16)"/>
        <column name="pi_id_tokenid" type="char(16)"/>
        <column name="primary_pi_id_tokenid" type="char(16)"/>
    </createTable>
</changeSet>

</databaseChangeLog>

