        <column name="Issuance_cd" type="char(1)"/>
        <column name="issuance_dt" type="DATETIME"/>
        <column name="workstation_name_tx" type="char(18)"/>
        <column name="Operator_CD" type="char(2)"/>
        <column name="Barcode_Type_CD" type="char(1)"/>
        <column name="file_sent" type="char(1)"/>
        <column name="Postage_Billed" type="char(1)"/>
