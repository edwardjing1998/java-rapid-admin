        <column name="wk_phone" type="char(14)"/>
        <column name="entity_cd" type="char(1)"/>
        <column name="role_cd" type="char(2)"/>
        <column name="pi_status" type="char(1)"/>
        <column name="status" type="char(1)"/>
        <column name="active" type="bit">
            <constraints nullable="false"/>
        </column>
        <column name="reason" type="char(1)"/>
        <column name="subreason" type="int"/>
        <column name="disposition" type="char(1)"/>
        <column name="in_hour" type="int"/>
        <column name="in_date" type="DATETIME"/>
